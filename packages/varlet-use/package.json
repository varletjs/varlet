{
  "name": "@varlet/use",
  "version": "3.6.1",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "description": "composable utils of varlet",
  "keywords": [
    "composable",
    "utils",
    "varlet"
  ],
  "author": "haoziqaq <357229046@qq.com>",
  "license": "MIT",
  "files": [
    "lib",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/varletjs/varlet.git"
  },
  "bugs": {
    "url": "https://github.com/varletjs/varlet/issues"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --out-dir=lib --watch --dts",
    "build": "tsup src/index.ts --format esm,cjs --out-dir=lib --dts --clean"
  },
  "dependencies": {
    "@varlet/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "tsup": "7.2.0",
    "typescript": "^5.1.5",
    "vue": "3.4.21"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}